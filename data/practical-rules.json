{
  "rules": [
    {
      "rule": "Words ending in '-ant'/'-ent' converts to '-nte'",
      "examples": [
        {
          "english": "Different",
          "spanish": "Diferente"
        },
        {
          "english": "Constant",
          "spanish": "Constante"
        }
      ]
    },
    {
      "rule": "Words ending in '-ly' converts to 'mente'",
      "examples": [
        {
          "english": "Constantly",
          "spanish": "Constantemente"
        },
        {
          "english": "Really",
          "spanish": "Realmente"
        }
      ]
    },
    {
      "rule": "Words ending in '-ence'/'-ance' converts to '-cia'",
      "examples": [
        {
          "english": "Compliance",
          "spanish": "Compliancia"
        },
        {
          "english": "Influence",
          "spanish": "Influencia"
        }
      ]
    },
    {
      "rule": "Words ending in '-ary' converts to '-ario'.",
      "examples": [
        {
          "english": "Necessary",
          "spanish": "Necessario"
        },
        {
          "english": "Contrary",
          "spanish": "Contrario"
        }
      ]
    },
    {
      "rule": "Words ending in '-ible'/'-able' are pronounced as 'bleh'",
      "examples": [
        {
          "english": "Possible",
          "spanish": "Posible"
        }
      ]
    },
    {
      "rule": "Words ending in '-tion' converts to '-ción'",
      "examples": [
        {
          "english": "Confirmation",
          "spanish": "Confirmación"
        }
      ]
    },
    {
      "rule": "The first 'R' in a word is usually a rolling 'R'",
      "examples": [
        {
          "english": "Real",
          "spanish": "Real"
        }
      ]
    },
    {
      "rule": "Double 'r' in a word is a rolling 'r'",
      "examples": [
        {
          "english": "Dog",
          "spanish": "Perro"
        }
      ]
    },
    {
      "rule": "There are no words that have a double 's'",
      "examples": [
        {
          "english": "Possible",
          "spanish": "Posible"
        }
      ]
    },
    {
      "rule": "'h's are silent unless it is 'ch'",
      "examples": [
        {
          "english": "Hospital",
          "spanish": "Hospital"
        },
        {
          "english": "Puppy",
          "spanish": "Cachorro"
        }
      ]
    },
    {
      "rule": "'z's are pronounced as 'th' in Spain or 's' in Latin America",
      "examples": [
        {
          "english": "Organize",
          "spanish": "Organizo"
        }
      ]
    },
    {
      "rule": "Past tense words have accents at the end",
      "examples": [
        {
          "english": "I tried",
          "spanish": "Inten'to'"
        }
      ]
    },
    {
      "rule": "'ar' verbs will lose their 'ar' and replace it with 'ado' for past tense versions of the verbs",
      "examples": [
        {
          "english": "I have spoken",
          "spanish": "He hablado"
        },
        {
          "english": "We have anticipated it for a long time",
          "spanish": "Lo hemos anticipado por mucho tiempo"
        },
        {
          "english": "Where have (did) you celebrated it?",
          "spanish": "Dondé lo has celebrado?"
        },
        {
          "english": "Has she taken it?",
          "spanish": "Lo ha tomado?"
        }
      ]
    },
    {
      "rule": "'ir/er' verbs will lose their 'ir/er' and replace it with 'ido' for past tense versions of the verbs",
      "examples": [
        {
          "english": "I have sold it",
          "spanish": "Lo he vendido"
        },
        {
          "english": "They have arrived",
          "spanish": "Han venido"
        },
        {
          "english": "We have lost",
          "spanish": "Hemos perdido"
        }
      ]
    },
    {
      "rule": "Present tense words have accents at the second last syllable",
      "examples": [
        {
          "english": "I try",
          "spanish": "In'ten'to"
        }
      ]
    },
    {
      "rule": "Future tense words can use present tense rules if given future context",
      "examples": [
        {
          "english": "I will try tomorrow",
          "spanish": "In'ten'to manána"
        },
        {
          "english": "I'm seeing her later",
          "spanish": "La veo mas tardé"
        },
        {
          "english": "Are you seeing them (female) soon?",
          "spanish": "Las ves pronto?"
        },
        {
          "english": "We are going to see her",
          "spanish": "La vamos a ver"
        },
        {
          "english": "Are you going to see him soon?",
          "spanish": "Lo vas a ver pronto?"
        },
        {
          "english": "Are you going to see us soon?",
          "spanish": "Nos vas a ver pronto?"
        },
        {
          "english": "Let's speak next week -> Let's speak the week that (it) comes",
          "spanish": "Hablamos la semana qué viene"
        },
        {
          "english": "We see each other -> We see us -> Goodbye",
          "spanish": "Nos vemos"
        },
        {
          "english": "We will see each other next week",
          "spanish": "Nos vemos la semana qué viene"
        }
      ]
    },
    {
      "rule": "No tense words have no accent",
      "examples": [
        {
          "english": "To try",
          "spanish": "Intentar"
        }
      ]
    },
    {
      "rule": "'ar' verbs are the biggest group. New verbs adopt 'ar'",
      "examples": [
        {
          "english": "To Twitter",
          "spanish": "Twittar"
        }
      ]
    },
    {
      "rule": "For nouns you can put 'a' ('una') or 'the' ('la') in front of it",
      "examples": [
        {
          "english": "A dog",
          "spanish": "Una perro"
        },
        {
          "english": "The cat",
          "spanish": "La gato"
        }
      ]
    },
    {
      "rule": "For adjectives you can put 'is' ('es') in front of it",
      "examples": [
        {
          "english": "He is tall",
          "spanish": "El es alto"
        }
      ]
    },
    {
      "rule": "Adjectives are words that describe a word. Adjectives come after the noun they describe",
      "examples": [
        {
          "english": "A digital camera",
          "spanish": "Una camera digital"
        },
        {
          "english": "I want to buy a digital camera",
          "spanish": "Quiero comprar una camera digital"
        },
        {
          "english": "The big car",
          "spanish": "El carro grande"
        }
      ]
    },
    {
      "rule": "Adjectives become plural when a noun is plural",
      "examples": [
        {
          "english": "The big houses",
          "spanish": "Las casas grandes"
        },
        {
          "english": "The big cars",
          "spanish": "Los autos grandes"
        },
        {
          "english": "The big cars",
          "spanish": "Los carros grandes"
        }
      ]
    },
    {
      "rule": "'The' (masculine) is 'El'. 'The' (feminine) is 'La'",
      "examples": [
        {
          "english": "The camera",
          "spanish": "La camera"
        },
        {
          "english": "The car",
          "spanish": "El coche (other ways to say car are: carro, auto, and machina)"
        }
      ]
    },
    {
      "rule": "'The' (plural masculine) is 'Los'. 'The' (plural feminine) is 'Las'",
      "examples": [
        {
          "english": "The cars",
          "spanish": "Los carros"
        },
        {
          "english": "The cars",
          "spanish": "Las machinas"
        }
      ]
    },
    {
      "rule": "'A' (masculine) is 'un'. 'A' (feminine) is 'una'",
      "examples": [
        {
          "english": "A camera",
          "spanish": "Una camera"
        },
        {
          "english": "A car",
          "spanish": "Un coche (other ways to say car are: carro, auto, and machina)"
        }
      ]
    },
    {
      "rule": "Nouns have genders. The object itself does not have gender",
      "examples": [
        {
          "english": "A/The car",
          "spanish": "Un/El coche"
        },
        {
          "english": "A/The car",
          "spanish": "Una/La machina"
        },
        {
          "english": "A/The car",
          "spanish": "Un/El carro"
        }
      ]
    },
    {
      "rule": "Functional object words (pronouns) should come before conjugated verbs",
      "examples": [
        {
          "english": "I want 'it'",
          "spanish": "'Lo' quiero"
        }
      ]
    },
    {
      "rule": "Functional object words (pronouns) can come before or after 'to' form verbs. Putting the pronoun before is more common.",
      "examples": [
        {
          "english": "I want to see 'it'",
          "spanish": "Quiero ver lo"
        },
        {
          "english": "I want to see 'it'",
          "spanish": "Lo quiero ver"
        }
      ]
    },
    {
      "rule": "Functional subject words (pronouns) are flexible to be placed in the beginning, middle or end of a sentence. Depends on dialect and region",
      "examples": [
        {
          "english": "Are you giving it to me tomorrow? (You give me it tomorrow?)",
          "spanish": "Usted me lo da manãna?"
        },
        {
          "english": "Are you giving it to me tomorrow? (You give me it tomorrow?)",
          "spanish": "Me lo da usted manãna?"
        },
        {
          "english": "Are you giving it to me tomorrow? (You give me it tomorrow?)",
          "spanish": "Me lo da manãna usted?"
        }
      ]
    },
    {
      "rule": "Pronoun object group: lo, la, te, nos, los, las, me",
      "examples": [
        {
          "english": "I want to see you",
          "spanish": "Te quiero ver"
        }
      ]
    },
    {
      "rule": "Pronoun subject group: el, ella, tú, nosotros, nosotras, usted, yo, ellos, ellas",
      "examples": [
        {
          "english": "They want to see us",
          "spanish": "Ellos nos quieren ver"
        },
        {
          "english": "Do you have time to talk?",
          "spanish": "Usted tenga tiempo hablar?"
        }
      ]
    },
    {
      "rule": "Usted comes from 'Vuestra Merced' which means 'Your mercy'",
      "examples": [
        {
          "english": "Do you know where to go",
          "spanish": "Usted sabe dondé ir?"
        }
      ]
    },
    {
      "rule": "Feminine words end in 'a' or 'ion'",
      "examples": [
        {
          "english": "The house",
          "spanish": "La casa"
        },
        {
          "english": "The opinion",
          "spanish": "La opinion"
        }
      ]
    },
    {
      "rule": "Words ending in 'ma' are masculine",
      "examples": [
        {
          "english": "The problem",
          "spanish": "El problema"
        },
        {
          "english": "Have you understood the problem?",
          "spanish": "Usted ha entendido el problema?"
        }
      ]
    },
    {
      "rule": "Words ending in 'ma' are derived from Greek. In Greek, these words are neutral.",
      "examples": [
        {
          "english": "The system",
          "spanish": "El systema"
        },
        {
          "english": "Have you understood the system?",
          "spanish": "Has entendido el systema?"
        },
        {
          "english": "The climate",
          "spanish": "El clima"
        }
      ]
    },
    {
      "rule": "Use 'me' as 'myself' to express 'I got' or 'I get'",
      "examples": [
        {
          "english": "I get ready (I prepare myself)",
          "spanish": "Me preparo"
        },
        {
          "english": "I got lost / I've got lost (I lost myself)",
          "spanish": "Me he perdido"
        },
        {
          "english": "I have lost myself (being emphatic)",
          "spanish": "Yo me he perdido"
        }
      ]
    },
    {
      "rule": "Use 'te' as 'yourself' to express 'you got' or 'you get'",
      "examples": [
        {
          "english": "You have got lost (You have lost yourself)",
          "spanish": "Te has perdido"
        },
        {
          "english": "You have got lost (being emphatic)",
          "spanish": "Tú te has perdido"
        },
        {
          "english": "You have lost (informal)",
          "spanish": "Has perdido"
        },
        {
          "english": "You have lost (being emphatic)",
          "spanish": "Tú has perdido"
        }
      ]
    },
    {
      "rule": "Use 'nos' as 'ourselves' to express 'we got' or 'we get'",
      "examples": [
        {
          "english": "We have got lost (We have lost ourselves)",
          "spanish": "Nos hemos perdido"
        },
        {
          "english": "We have got lost (being emphatic)",
          "spanish": "Nosotros nos hemos perdido"
        },
        {
          "english": "We have lost",
          "spanish": "Hemos perdido"
        },
        {
          "english": "We have lost (being emphatic)",
          "spanish": "Nosotros hemos perdido"
        }
      ]
    },
    {
      "rule": "Words ending in '-ity' converts to '-idad'. These words are feminine",
      "examples": [
        {
          "english": "The diversity",
          "spanish": "La diversidad"
        },
        {
          "english": "The clarity",
          "spanish": "La claridad"
        }
      ]
    },
    {
      "rule": "'ir' and 'er' verbs expand conjugation by using 'e' as its base. These words are historic and can have irregularity when conjugating",
      "examples": [
        {
          "english": "To sell",
          "spanish": "Vend'er'"
        },
        {
          "english": "I sell",
          "spanish": "Vend'o'"
        },
        {
          "english": "He/She/It/You(formal) sells",
          "spanish": "Vend'e'"
        },
        {
          "english": "They/You(plural) sells",
          "spanish": "Vend'en'"
        },
        {
          "english": "You(informal) sells",
          "spanish": "Vend'es'"
        },
        {
          "english": "We sell",
          "spanish": "Vend'emos'"    
        }
      ]
    },
    {
      "rule": "'ar' verbs expand conjugation by using 'a' as its base",
      "examples": [
        {
          "english": "To collaborate",
          "spanish": "Collabor'ar'"
        },
        {
          "english": "I collaborate",
          "spanish": "Collabor'o'"
        },
        {
          "english": "He/She/It/You(formal) collaborates",
          "spanish": "Collabor'a'"
        },
        {
          "english": "They/You(plural) collaborate",
          "spanish": "Collabor'an'"
        },
        {
          "english": "You(informal) collaborates",
          "spanish": "Collabor'as'"
        },
        {
          "english": "We collaborate",
          "spanish": "Collabor'amos'"
        }
      ]
    },
    {
      "rule": "Use 'a' to express '-ing' words before the 'to' form of a verb",
      "examples": [
        {
          "english": "I am going out to eat",
          "spanish": "Salgo a comer"
        },
        {
          "english": "When are you coming to eat?",
          "spanish": "Cuando vienes a comer?"
        }
      ]
    },
    {
      "rule": "When repeating an expression after a conjunction, you can shorten it with 'si'",
      "examples": [
        {
          "english": "I don't want to invite him but she wants to invite him (as in 'she does')",
          "spanish": "Yo no tengo invitar lo pero ella si"
        }
      ]
    },
    {
      "rule": "The first 'e' in 'to' form present tense verbs typically will split as 'ie'",
      "examples": [
        {
          "english": "Tempo",
          "spanish": "T'ie'mpo"
        },
        {
          "english": "'To want' / 'You want'",
          "spanish": "'Querer' / 'Quieres'"
        }
      ]
    },
    {
      "rule": "The first 'o' in 'to' form present tense verbs typically will split as 'ue'",
      "examples": [
        {
          "english": "'To put' / 'I put'",
          "spanish": "'Poder' / 'Puedo'"
        }
      ]
    },
    {
      "rule": "A verb is used as a noun when 'a' or 'the' is in front of the word",
      "examples": [
        {
          "english": "An encounter",
          "spanish": "Un encontrar"
        },
        {
          "english": "His encounter",
          "spanish": "El encontrar"
        }
      ]
    },
    {
      "rule": "Words starting in 'al' are typically from the Arabic language",
      "examples": [
        {
          "english": "Alcohol",
          "spanish": "'Al'cohol"
        },
        {
          "english": "Algebra",
          "spanish": "'Al'gebra"
        }
      ]
    },
    {
      "rule": "If a words ends in a consonant, you add 'es' (not just 's')",
      "examples": [
        {
          "english": "You guys",
          "spanish": "Ustedes"
        }
      ]
    },
    {
      "rule": "You clarify saying 'you guys' as opposed to 'they/them' with ustedes",
      "examples": [
        {
          "english": "Are you guys give it to us tomorrow? (You guys give us it tomorrow?)",
          "spanish": "Ustedes nos lo dan manãna?"
        }
      ]
    },
    {
      "rule": "Prepositions are usually built into the words such as me, te, and nos so they could be removed from the sentence",
      "examples": [
        {
          "english": "He is speaking to me",
          "spanish": "Me habla"
        },
        {
          "english": "They speak to me",
          "spanish": "Me hablan"
        },
        {
          "english": "They speak to me about it (They speak me it)",
          "spanish": "Me lo hablan"
        },
        {
          "english": "He buys it for me (He buys me it)",
          "spanish": "Me lo compra"
        },
        {
          "english": "Buy it from me",
          "spanish": "Me lo compra"
        },
        {
          "english": "I buy it for you (I buy you it)",
          "spanish": "Te lo compro"
        },
        {
          "english": "Shall I buy it for you? (I buy you it?)",
          "spanish": "Te lo compro?"
        },
        {
          "english": "Do you want to do it for me? (You want to do me it?)",
          "spanish": "Me lo quieres hacer?"
        }
      ]
    },
    {
      "rule": "Use pronoun verb + 'to' verb sentence structure to your advantage: voy, doy, puedo, vengo, quiero (querer), quedo (quedar), hago (hacer), he (haber), tengo qué, debo, gusto, etc",
      "examples": [
        {
          "english": "I have to go",
          "spanish": "Tengo qué ir"
        },
        {
          "english": "I can't find it (as opposed to I don't find it)",
          "spanish": "No lo puedo encontrar (No lo encuentro)"
        },
        {
          "english": "I am staying in the red car (I remain myself in the car red)",
          "spanish": "Me quedo en el auto rojo"
        },
        {
          "english": "I have stayed in the red car (I remained myself in the car red)",
          "spanish": "Me he quedado en el auto rojo"
        },
        {
          "english": "I have bought a red car (I bought a car red)",
          "spanish": "He comprado un auto rojo"
        },
        {
          "english": "I have bought myself a red car (I bought myself a car red)",
          "spanish": "Me he comprado un auto rojo"
        }
      ]
    }
  ]
}